{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "sourceRoot": "",
    "outDir": "./build",

    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit":false,
    "inlineSourceMap": true,
    "jsx": "preserve",
    "baseUrl": "",
    "paths": {
      "*": ["*", "./src/*"],
      "@mc-app-services": ["C:/Appl/Source/Equinor/procosys-mc-webapp/src/services/*"],
      "@mc-app-utils": ["C:/Appl/Source/Equinor/procosys-mc-webapp/src/until/*"]
      
    }
  },
  "include": [
    "src",
    "src/specs/**/*.ts"
  ]
}
